    "@types/jest": "^29.5.12",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.12",
    "jest": "^30.0.0-alpha.6",
    "jest-environment-jsdom": "^29.7.0",
